<!DOCTYPE HTML>
<html lang="fr">
<head>
    <!--=============== basic  ===============-->
    <meta charset="UTF-8">
    <title>{% block title %}- AWA Partners{% endblock %}</title>
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="robots" content="index, follow"/>
    <meta name="keywords" content=""/>
    <meta name="description" content=""/>
    <!--=============== css  ===============-->
    {% block css %}
        {% stylesheets output="css/compiled/mainfrontcss.css" filter="cssrewrite"
        'bundles/awapartners/frontAssets/css/reset.css'
        'bundles/awapartners/frontAssets/css/plugins.css'
        'bundles/awapartners/frontAssets/css/style.css' %}
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>
        {% endstylesheets %}
        <!--=============== favicons ===============-->
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    {% endblock %}
</head>
<body>
<!-- loader -->
<div class="loader">
    <div id="movingBallG">
        <div class="movingBallLineG"></div>
        <div id="movingBallG_1" class="movingBallG"></div>
    </div>
</div>
<!-- loader end -->
<!--================= main start ================-->
<div id="main">
    <!--=============== header ===============-->
    <header>
        <!-- header-inner  -->
        <div class="header-inner">
            <!-- header logo -->
            <div class="logo-holder">
                <a href="{{ path('home') }}"><img src="{{ asset('awa-rouge.png') }}" alt="AWA Partners logo"></a>
            </div>
            <!-- header logo end -->
            <!-- mobile nav button -->
            <div class="nav-button-holder">
                <div class="nav-button vis-m"><span></span><span></span><span></span></div>
            </div>
            <!-- mobile nav button end -->
            <!-- navigation  -->
            <div class="nav-holder">
                {% set current_route_name = app.request.attributes.get('_route') %}
                <nav>
                    <ul>
                        <li>
                            <a style="font-size: 14px" href="{{ path('home') }}"
                               {% if current_route_name == 'home' %}class="act-link" {% endif %}>Accueil</a>
                        </li>
                        <li>
                            <a style="font-size: 14px" href="{{ path('about_us') }}"
                               {% if current_route_name == 'about_us' %}class="act-link" {% endif %}>A propos de
                                nous</a>
                        </li>
                        <li>
                            <a style="font-size: 14px" href="{{ path('project_index') }}"
                               {% if current_route_name == 'project_index' or current_route_name == 'project_show' %}class="act-link" {% endif %} >Projets</a>
                        </li>
                        <li>
                            <a style="font-size: 14px" href="{{ path('client_list') }}"
                               {% if current_route_name == 'client_list' %}class="act-link" {% endif %} >Partenaires</a>
                        </li>
                        <li>
                            <a style="font-size: 14px" href="{{ path('application_new') }}"
                               {% if current_route_name == 'application_new' %}class="act-link" {% endif %}>Carrière</a>
                        </li>
                        <li>
                            <a style="font-size: 14px" href="{{ path('contact_index') }}"
                               {% if current_route_name == 'contact_index' %}class="act-link" {% endif %}>Contactez
                                nous</a>
                        </li>
                    </ul>
                </nav>
            </div>
            <!-- navigation  end -->
        </div>
        <!-- header-inner  end -->
    </header>
    {% block body %}
        {% block footer %}
            <!-- content footer-->
            <div class="height-emulator"></div>
            <footer class="content-footer">
                <!--  container  -->
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            <!-- Footer logo -->
                            <div class="footer-item footer-logo">
                                <a href="{{ path('home') }}"><img src="{{ asset('awa-rouge.png') }}" alt="AWA Partners logo"></a>
                               <p >We build excellence.</p>
                            </div>
                            <!-- Footer logo end -->
                        </div>
                        <!-- Footer info -->
                        <div class="col-md-2">
                            <div class="footer-item">
                                <h4 class="text-link">Numéros</h4>
                                <ul>
                                    <li><span>Tél: (+216)71 965 291</span></li>
                                    <li><span>Fax: (+216)71 965 378</span></li>
                                </ul>
                            </div>
                        </div>
                        <!-- Footer info end-->
                        <!-- Footer info -->
                        <div class="col-md-2">
                            <div class="footer-item">
                                <h4 class="text-link">Email</h4>
                                <ul>
                                    <li><span>contact@awa.com.tn</span></li>
                                </ul>
                            </div>
                        </div>
                        <!-- Footer info-->
                        <!-- Footer info end-->
                        <div class="col-md-2" >
                            <div class="footer-item">
                                <h4 class="text-link">Adresse</h4>
                                <ul style="margin-left: 5px">
                                    <li><span>Angle rue Lac Toba et rue Lac Malaren,
                                            Imm. Mezghanni Appt 3, Etg 2,
                                            1053 les Berges du Lac
                                        </span>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <!-- Footer info end-->
                    </div>
                    <!-- Footer copyright -->
                    <div class="row">
                        <div class="col-md-6"></div>
                        <div class="col-md-6">
                            <div class="footer-wrap">
                                        <span class="copyright">  &#169;  <script> document.write(new Date().getFullYear()); </script>
                                             - powred by <a style="color: #fff; border-color: #fff; border-bottom: solid 1px" target="_blank" href="http://www.sbc.tn">Smart Business Challenge</a>
                                        </span>
                                <span class="to-top">Allez en haut</span>
                            </div>
                        </div>
                    </div>
                    <!-- Footer copyright end -->
                </div>
                <!--  container  end -->
                <!-- Hover animation  -->
                <canvas class="particular"></canvas>
                <!-- Hover animation  end -->
            </footer>
            <!-- content footer end -->
            <!-- share  -->
            <div class="share-inner">
                <div class="share-container  isShare" data-share="['facebook','googleplus','twitter','linkedin']"></div>
                <div class="close-share"></div>
            </div>
            <!-- share end -->
        {% endblock %}
    {% endblock %}
    <!-- Fixed footer -->
    <footer class="fixed-footer">
        <div class="footer-social">
            <ul>
                <li><a href="#" target="_blank"><i class="fa fa-facebook"></i></a></li>
                <li><a href="#" target="_blank"><i class="fa fa-google-plus"></i></a></li>
                <li><a href="#" target="_blank"><i class="fa fa-linkedin"></i></a></li>
            </ul>
        </div>
        <!-- Header  title -->
        <div class="footer-title">
            <h2><a href="#" style="font-size: 12px"></a></h2>
        </div>
        <!-- Header  title  end-->
    </footer>
    <!-- Fixed footer end-->
</div>
<!-- Main end -->
<!--=============== scripts  ===============-->
{% block js %}
    {% javascripts output="js/compiled/mainfrontjs.js"
    'bundles/awapartners/frontAssets/js/jquery.min.js'
    'bundles/awapartners/frontAssets/js/plugins.js'
    'bundles/awapartners/frontAssets/js/scripts.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
</body>
</html>